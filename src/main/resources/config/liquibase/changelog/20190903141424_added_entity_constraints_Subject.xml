<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Subject.
    -->
    <changeSet id="20190903141424-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="prod_service_id"
                                 baseTableName="subject"
                                 constraintName="fk_subject_prod_service_id"
                                 referencedColumnNames="id"
                                 referencedTableName="prod_service"/>

        <addForeignKeyConstraint baseColumnNames="subject_description_id"
                                 baseTableName="subject"
                                 constraintName="fk_subject_subject_description_id"
                                 referencedColumnNames="id"
                                 referencedTableName="occurrence"/>

    </changeSet>
</databaseChangeLog>
