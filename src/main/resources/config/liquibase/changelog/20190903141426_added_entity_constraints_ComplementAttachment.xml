<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity ComplementAttachment.
    -->
    <changeSet id="20190903141426-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="attachment_id"
                                 baseTableName="complement_attachment"
                                 constraintName="fk_complement_attachment_attachment_id"
                                 referencedColumnNames="id"
                                 referencedTableName="attachment"/>

        <addForeignKeyConstraint baseColumnNames="complement_id"
                                 baseTableName="complement_attachment"
                                 constraintName="fk_complement_attachment_complement_id"
                                 referencedColumnNames="id"
                                 referencedTableName="complement"/>

    </changeSet>
</databaseChangeLog>
